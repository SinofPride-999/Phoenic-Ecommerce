    @apply bg-transparent text-foreground border-2 border-border hover:border-primary;
    @apply transition-all duration-300 transform hover:scale-105;
  }
  
  /* Product Card Effects */
  .product-card {
    @apply relative overflow-hidden rounded-2xl bg-card shadow-soft;
    @apply transition-all duration-300 hover:shadow-large hover:-translate-y-1;
    background: var(--gradient-card);
  }
  
  .product-card::before {
    content: '';
    @apply absolute inset-0 opacity-0 transition-opacity duration-300;
    background: linear-gradient(135deg, transparent 0%, hsl(var(--primary) / 0.05) 100%);
